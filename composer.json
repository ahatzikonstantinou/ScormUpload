{
    "name": "ahat/scormupload",
    "description": "Library to check uploaded scorm zip packages for viruses and storing (unzipped) in google cloud storage",
    "type": "library",
    "authors": [
        {
            "name": "Antonis Hatzikonstantinou",
            "email": "ahatzikonstantinou@gmail.com"
        }
    ],
    "require": {
        "google/cloud-storage": "^1.7",
        "xenolope/quahog": "^2.1"
    },  
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {  
        "psr-4": {  
            "ahat\\ScormUpload\\": "src"  
        }  
    },  
    "autoload-dev": {  
        "psr-4": {  
            "ahat\\ScormUpload\\Tests\\": "tests/"  
        }  
    },
    "config": {
    	"optimize-autoloader": true
    }
}
